{
    "title": "YUNLIHUB",
    "author": "Yunli",
    "description": "有光就读诗，无光就做梦",
    "language": "zh-hans",
    "links": {
        "sidebar": {
            "Home": "https://www.yunlihub.com",
            "Blog": "https://www.yunlihub.top"
        }
    },
    "plugins": [
        "back-to-top-button",
        "expandable-chapters",
        "theme-comscore",
        "splitter",
        "alerts",
        "chapter-fold",
        "code",
        "github",
        "favicon",
        "accordion",
        "-lunr",
        "-search",
        "search-pro",
        "emphasize",
        "-sharing",
        "sharing-plus",
        "toc",
        "tbfed-pagefooter",
        "lightbox",
        "flexible-alerts",
        "pageview-count",
        "hide-element"
    ],
    "pluginsConfig": {
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "github": {
            "url": "https://github.com/yunliyo"
        },
        "tbfed-pagefooter": {
            "copyright": "Yunli",
            "modify_label": "文件更新时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": ["a.gitbook-link[href='https://www.gitbook.com']"]
        }

    }
}